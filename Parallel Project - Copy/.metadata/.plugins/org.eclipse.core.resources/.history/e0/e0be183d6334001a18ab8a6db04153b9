insertQuery = insert into customer values(?,?,?,?,?,?,?,?)
deleteQuery = delete from customer where customerId=?
updateQuery = update customer set customername=?,streetaddress1=?,streetaddress2=?,town=?,postalcode=?,email=?,telephone=? where customerid=?
insertQuery1 = insert into product values(?,?,?) 
ListAll = select * from customer
insertQuery2 = insert into product values(?,?,?)
deleteQuery2 = delete from product where productId=?
updateQuery2 = update product set productName=?,deliveryDate=? where productId=?
insertQuery3 = insert into contract values(?,?,?,?,?,?,?)
deleteQuery3 = delete from contract where contractNo=?
updateQuery3 = update contract set customerid=?,productid=?,haulierid=?,deliverydate=?,deliveryday=?,quantity=? where contractno=?
ListAll3=select * from contract;
